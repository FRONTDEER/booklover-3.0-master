}

.showloading {
  position: fixed;
  text-align: center;
  opacity: 0;
  transition: 0.3s;
  top: 100%;
  width: 100%;
  height: 100%;
  z-index: 100;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-flow: column;
}

.showloading.load {
  opacity: 1;
  transition: 0.3s;
  top: 0;
}

.showloading .loadicon {
  width: 100rpx;
  height: 100rpx;
  animation: turn 0.4s linear infinite;
  position: relative;
  z-index: 10;
  opacity: 0.6;
  font-size: 2.3em;
}

.showloading .loadtext {
  display: flex;
  align-items: center;
  justify-content: center;
}

.loadtext .text {
  font-size: 28rpx;
  opacity: 0.5;
}

.bgline {
  background-image: linear-gradient(135deg, rgba(255, 255, 255, 0.1), rgba(255, 255, 255, 0.1) 25%, rgba(255, 255, 255, 0) 25%, rgba(255, 255, 255, 0) 50%, rgba(255, 255, 255, 0.1) 50%, rgba(255, 255, 255, 0.1) 75%, rgba(255, 255, 255, 0) 75%, rgba(255, 255, 255, 0) 100%);
  background-size: 300rpx 300rpx;
}

@keyframes turn {
  0% {
    -webkit-transform: rotate(0deg);
  }

  25% {
    -webkit-transform: rotate(90deg);
  }

  50% {
    -webkit-transform: rotate(180deg);
  }

  75% {
    -webkit-transform: rotate(270deg);
  }

  100% {
    -webkit-transform: rotate(360deg);
  }
}

