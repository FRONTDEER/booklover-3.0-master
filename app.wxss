.tabwrapper {
  position: relative;
  z-index: 100;
}

.tabwrapperbg,
.subwrapper {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

.tabwrapper .tabwrapperbg {
  z-index: 1;
}

.tabwrapper .subwrapper {
  z-index: 2;
}

.tabwrapper .mainwrapper {
  position: relative;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 3;
  transition: .3s;
}

.subwrapper .close .btn {
  width: 88rpx;
  position: relative;
  z-index: 10;
  display: flex;
  justify-content: space-around;
  align-items: center;
  overflow: hidden;
  margin-left: 20rpx;
}

.subwrapper .close .btn .icon {
  align-items: center;
  width: 50rpx;
  height: 50rpx;
  position: relative;
  z-index: 15;
  overflow: visible;
}

.tabwrapper.on .mainwrapper {
  position: fixed;
  border-radius: 20rpx;
  transition: .3s;
  transform: scale(1);
  overflow: hidden;
  left: 75%;
}

.tabwrapper .tabbar {
  position: fixed;
  width: 100%;
  bottom: 0rpx;
  left: 0;
  z-index: 100;
  transition: 1s;
  transform-origin: 50% 200% 0;
}

.tabwrapper.on .tabbar {
  bottom: 0rpx;
  transition: .3s;
  transform: scale(.9);
  opacity: .95;
  transform-origin: 0 100% 0;
}

.tabwrapper .mainwrapper::after {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 1);
  opacity: 0;
  z-index: 999;
  pointer-events: none;
  transition: .8s;
}

.tabwrapper.on .mainwrapper::after {
  opacity: .3;
  transition: .3s;
  pointer-events: auto;
}


/* 公共样式 */
.margin {
