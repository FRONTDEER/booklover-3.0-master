  height: 138rpx;
  position: relative;
  float: left;
  border-radius: 5rpx;
  margin-right: 10rpx;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  background-color: rgba(120, 120, 120, 0.1);
}

.libcover::before {
  content: '';
  position: absolute;
  z-index: 10;
  width: 100%;
  height: 100%;
  opacity: 0.5;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: url("data:image/png;base64,");
  background-position: 0 0;
  background-repeat: repeat-y;
  background-size: contain;
}

.movcover::before {
  content: "";
  position: absolute;
  background-color: rgba(0, 0, 0, 0.1);
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.movcover::after {
  content: '';
  position: absolute;
  background-image: url(data:image/png;base64,);
  width: 40%;
  height: 40%;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
}

.postmsg {
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  top: -110rpx;
  left: 0;
  padding: 30rpx;
  z-index: 2;
}

.postmsg .data {
  display: flex;
  justify-content: center;
  align-items: center;
}

.postmsg .data .item {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 40rpx;
}

.postmsg .data .item:last-child {
  margin-right: 0rpx;
}

.postmsg .data .icon {
  width: 50rpx;
  height: 50rpx;
  font-size: 1.5em;
  opacity: .9;
  margin-right: 5rpx;
}

.postmsg .data .text {
  font-size: 30rpx;
  font-weight: 600;
  opacity: .9;
}

.postmsg .time {
  font-size: 24rpx;
  opacity: 0.4;
}

.cardimage .imagecover {
  position: relative;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: 50% 50%;
}

.cardimage .imagecover::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.cardimage .imagecover .video {
  background: rgba(0, 0, 0, 1);
}

.cardimage .imagecover .overlay {
  background: rgba(0, 0, 0, 0.5);
  backdrop-filter: blur(10px);
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

.cardvideo .imagecover::before {
  content: "";
  position: absolute;
  background-color: rgba(0, 0, 0, 0.2);
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.cardvideo .imagecover::after {
  content: '';
  position: absolute;
  background-image: url(data:image/png;base64,);
  width: 55px;
  height: 55px;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}

.fixedbox {
  position: fixed;
  bottom: -200rpx;
  left: 0;
  right: 0;
  padding: 20rpx 40rpx;
  z-index: 100;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.1);
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
  transition: 0.5s;
}

.fixedbox.fixed {
  transition: 0.5s;
  bottom: 0;
}

.fixedbox .box {
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 24rpx;
  overflow: hidden;
}

.fixedbox .box .btn {
  flex: 50%;
  height: 80rpx;
  margin-left: 10rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(123, 123, 123, .1);
  position: relative;
}

.fixedbox .box .btn .button {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border: none !important;
  opacity: 0;
  z-index: 15;
}

.fixedbox .box .btn:first-child {
  margin-left: 0rpx;
}

