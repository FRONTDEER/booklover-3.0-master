}

.navigate-booksmg .navigate-booksmg-txt text.tit {
  font-size: 30rpx;
  display: block;
  height: 40rpx;
  font-weight: bold;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.navigate-booksmg .navigate-booksmg-txt text.desc {
  display: block;
  font-size: 20rpx;
  height: 24rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.navigate-booksmg-box.active .navigate-booksmg {
  opacity: 1;
  top: 0rpx;
  transition: 0.3s;
}

