/*招财猫广告位*/

.floatads {
  position: fixed;
  right: -200rpx;
  bottom: calc(140rpx + env(safe-area-inset-bottom));
  transition: 0.5s;
  z-index: 100;
  opacity: 1;
}

.floatads.active {
  right: 30rpx;
  transition: 0.5s;
}

.floatads .image {
  width: 150rpx;
  animation: music-move 1s 0.5s infinite alternate ease-in-out;
  transform-origin: bottom;
  opacity: 1;
}

.floatads .image.transactive {
  opacity: 0.3;
}

@keyframes grow {
  0% {
    transform: scale(0);
  }

  60% {
    transform: scale(1.15);
  }

  80% {
    transform: scale(0.95);
  }

  100% {
    transform: scale(1);
  }
}

@keyframes music-move {
  0% {
    transform: rotate(0deg);
  }

  33% {
    transform: rotate(-5deg);
  }

  66% {
    transform: rotate(5deg);
  }

  100% {
    transform: rotate(0deg);
  }
}

.floatads .close {
  position: absolute;
  width: 100%;
  height: 60rpx;
