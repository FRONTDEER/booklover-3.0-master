

.welcomebox {
  position: fixed;
  z-index: -5;
}
.welcomebox.on {
  position: fixed;
  z-index: 101;
}
.catgoldarea {
  position: absolute;
  bottom: -40rpx;
  right: 40rpx;
}

.catgoldarea .catgold {
  position: absolute;
  bottom:0;
  right:0;
  width: 227rpx;
  height: 171rpx;
  transform-origin: 100% 100% 0;
  transform: scale(0.6);
}

.catgoldarea .catgold .image {
  width: 227rpx;
  height: 171rpx;
}

.catgoldarea .catgold .catgoldbody {
  position: absolute;
  left: 0;
  bottom: 16rpx;
  z-index: 10;
  height: 161rpx;
  overflow: hidden;
}

.catgoldarea .catgold .catgoldact {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 11;
}

.catgoldarea .catgold .catgoldact .image {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 11;
  /* animation: ani1 0.5s 0.3s infinite alternate ease-in-out; */
  background-size: cover;
}

@keyframes ani1 {

  0%,
  40% {
    background-image: url(data:image/png;base64,);
    background-size: cover;
  }

  55%,
  100% {
    background-image: url(data:image/png;base64,);
    background-size: cover;
  }
}

.catgoldarea .cattip {
  position: relative;
  right: 170rpx;
  bottom: 46rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #d8b147;
  border-radius: 10rpx;
  text-align: center;
  font-size: 30rpx;
}

.catgoldarea .cattip .text {
  color: rgba(255, 255, 255, 1);
  padding:0 20rpx;
  display: -webkit-box;
  overflow: hidden;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  max-width:300rpx;
}

.catgoldarea .cattip::after {
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  top: 10rpx;
  right: -40rpx;
  border-style: solid;
  border-width: 15rpx 20rpx 15rpx 30rpx;
  border-color: transparent transparent transparent #d8b147;
  font-size: 0;
}


.swiper {
  height: 350rpx !important;
}

